{
  "name": "luminote",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered Christmas light sequence generator",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "npm run build && wrangler pages deploy ./out --project-name=luminote",
    "test:prompt": "node -r dotenv/config node_modules/.bin/tsx scripts/test-prompt-parser.ts dotenv_config_path=.env.local",
    "test:effects": "npx tsx scripts/test-effect-renderer.ts",
    "parse-layout": "npx tsx scripts/parse-layout.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "fast-xml-parser": "^4.5.0",
    "next": "^15.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "^15.1.3",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "wrangler": "^3.100.0"
  }
}
